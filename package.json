{
  "name": "vamo-heatos",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "npm-run-all2 --parallel dev:web dev:api",
    "dev:web": "pnpm --filter web dev",
    "dev:api": "pnpm --filter api start:dev",
    "start:prod": "npm-run-all2 --parallel start:prod:web start:prod:api",
    "start:prod:web": "pnpm --filter web preview --host",
    "start:prod:api": "pnpm --filter api start:prod",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck"
  },
  "dependencies": {
    "@vamo-heatos/shared": "workspace:^"
  },
  "devDependencies": {
    "npm-run-all2": "^8.0.4"
  }
}
